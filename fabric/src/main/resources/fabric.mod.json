    ],
    "preLaunch": [
      "net.takeoff.fabric.ValkyrienPreLaunch"
    ],
    "modmenu": [
      "net.takeoff.fabric.TakeoffModFabric${'$'}ModMenu"
    ]
  },
  "mixins": [
